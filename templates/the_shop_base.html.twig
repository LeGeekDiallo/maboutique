        <a href="{{ path('shop_page', {
            shop:theShop.id, slug:theShop.getSlug()
        }) }}"><span>{{ theShop.shopName }}</span></a>
    </div>
    <div class="__the_shop_nav_links">
        <a href="{{ path('cart', { shop:theShop.id }) }}" class="cart"><i class="fas fa-shopping-basket"></i>
            <span class="badge badge-danger cart_badge">{{ products.productInCart(theShop)|length }}</span>
        </a>
        {% if app.user %}
            <div class="client">
                <i class="fas fa-user-circle"></i><span class="__user_active"><i class="fas fa-circle"></i></span>
                <div class="__dropdown_content">
                    <ul class="list-unstyled">
                        <li>
                            <a href="{{ path('client') }}">Profil</a>
                        </li>
                        <li>
                            <a href="{{ path('app_logout') }}"><i class="fas fa-power-off"></i> Deconnexion</a>
                        </li>
                    </ul>
                </div>
            </div>
