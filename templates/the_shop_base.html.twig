            <div class="client"><i class="fas fa-user-circle"></i>
                <div class="__dropdown_content">
                    <ul class="list-unstyled">
                        <li>
                            <a href="{{ path('inscription') }}"><i class="fas fa-user-plus"></i> S'inscrire</a>
                        </li>
                        <li>
                            <a href="{{ path('login_user') }}"><i class="fas fa-sign-in-alt"></i> Se connecter</a>
                        </li>
                    </ul>
                </div>
            </div>
